{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.11" }
    }
  ],
  "routes": [
    {
      "src": "/analyze",
      "methods": ["POST","OPTIONS"],
      "dest": "main.py",
      "headers": { "x-vercel-protected": "0" }
    },
    {
      "src": "/analyze/file",
      "methods": ["POST","OPTIONS"],
      "dest": "main.py",
      "headers": { "x-vercel-protected": "0" }
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
